{
  "hooks": {},
  "git": {
    "changelog": "git log --pretty=format:\"* %s (%h)\" ${from}...${to}",
    "requireCleanWorkingDir": true,
    "requireBranch": false,
    "requireUpstream": true,
    "requireCommits": true,
    "addUntrackedFiles": false,
    "commit": false,
    "commitMessage": null,
    "commitArgs": [],
    "tag": true,
    "tagName": "v${version}",
    "tagMatch": null,
    "tagAnnotation": "Update of 'Форма оплаты' ${version}",
    "tagArgs": [],
    "push": true,
    "pushArgs": ["--follow-tags"],
    "pushRepo": ""
  }
}
